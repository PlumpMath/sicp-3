	)
	(echo ")")
	(+ x 1)
)
(define (p1dec x)
	(echo " (p1 ")
	(echo x)
	(echo "")
	(- x 1)
)
(define (p2inc x)
	(echo " (inc ")
	(echo x)
	(echo "))<br />")
	(+ x 1)
)
(define (p2dec x)
	(echo " (p2 (dec ")
	(echo x)
	(echo ")")
	(- x 1)
)
(define (p1 a b)
	(if (= a 0)
	b
	(p1inc (p1 (p1dec a) b)))
)
(define (p2 a b)
	(if (= a 0)
	b
	(p2 (p2dec a) (p2inc b)))
)
