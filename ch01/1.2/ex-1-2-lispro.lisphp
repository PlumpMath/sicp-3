(define eval*
	[macro (eval (car #arguments))])
(define cdr*
	[macro (car (cdr #arguments))])
(define car*
	[macro (car (car #arguments))])
(define cond*
	[macro (if(= 1 (eval (car (car #arguments))))
		  (eval (car (cdr (car #arguments))))
		  (if (= 1 (eval (car (car (cdr #arguments))))) 
  		  (eval (car (cdr (car (cdr #arguments))))) 
		  (if (= 1 (eval (car (car (cdr (cdr #arguments))))))
		  (eval (car (cdr (car (cdr (cdr #arguments))))))
		  (if (= 1 (eval (car (car (cdr (cdr (cdr #arguments))))))) (eval (car (cdr (car (cdr (cdr (cdr #arguments))))))) (eval (car (car (cdr (cdr (cdr (cdr #arguments)))))))))
)
)])
